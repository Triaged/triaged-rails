<div class="col-lg-12">
	<div class="spacer"></div>
	<div class="row">
		<div class="col-lg-6 col-lg-offset-3">
			<!-- Logo Row -->
			<div class="row">
				<%= link_to('/') do %><h2><%= image_tag "logotriage.png" %><% end %><span id="logo">Triage</span</h2>
			</div>
			<!-- Title Row -->
			<div class="row" id="welcome-title">
				<h1>FAQ</h1>
			</div>
			<!-- Description Row -->
			<div class="row" id="welcome-description">
				<p> Coming Soon.</p>
			</div>

		</div>
	</div>
</div>


<script type="text/javascript">
  var userAgent = window.navigator.userAgent;
  if (userAgent.match(/iPhone/i)) {
  	launchsockPicker = function() {
	       setTimeout(function() {
	        window.location = 'https://www.triaged.co'
	       }, 500);

	       window.location = 'triage://?<%=@token%>';
	   };

	   launchsockPicker();
  }
</script>
